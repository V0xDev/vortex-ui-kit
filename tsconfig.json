{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"],
      "@components/*": ["src/components/*"],
      "@stories/*": ["src/stories/*"]
    },
    "noEmit": true
  },
  "include": ["src/**/*.ts", "src/**/*.vue", "src/**/*.mdx"],
  "exclude": ["node_modules", "node_modules/@types/mdx"]
}
